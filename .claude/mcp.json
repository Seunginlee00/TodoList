{
  "version": "1.0",
  "mcp": {
    "servers": {
      "local-gemini": {
        "command": "node",
        "args": [
          "E:\\source\\mcp-server-gemini\\dist\\enhanced-stdio-server.js"
        ],
        "env": {
          "GEMINI_API_KEY": "${GEMINI_API_KEY}"
        }
      },
      "notion": {
        "command": "npx",
        "args": ["-y", "@suekou/mcp-notion-server"],
        "env": {
          "NOTION_API_TOKEN": "${NOTION_API_TOKEN}"
        }
      },
      "playwright": {
        "command": "npx",
        "args": ["-y", "@executeautomation/playwright-mcp-server"]
      }
    }
  }
}
